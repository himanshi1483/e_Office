@model IEnumerable<e_Office.Models.StandardHeadMaster>

@{
    ViewBag.Title = "Index";
}

    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                    <a class="btn btn-cyan float-right" href="@Url.Action("Create")">Create New</a>
                </div>
                <div class="card-body">
                    <h5 class="card-title m-b-0">Standard Head Master</h5>
                </div>
                <table class="table">
                    <tr>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { id = item.StdHeadId }) |
                                @Html.ActionLink("Delete", "Delete", new { id = item.StdHeadId })
                            </td>
                        </tr>
                    }

                </table>
                </div>
</div>
</div>